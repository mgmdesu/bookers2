<!--マイページ(Userページ)-->
<h2><strong>Books</strong></h2>

<table>
  <tr>
    <th>Title</th>
    <th>Opinion</th>
  </tr>

  <% @books.each do |book| %>
  <tr>
    <td><%= image_tag book.user.get_profile_image(100,100) %></td>
    <td><%= book.title %></td>
    <td><%= book.body %></td>
  </tr>
  <% end %>
</table>

