<!--User_infoのページ-->
<%= render 'books/user_info', book: @book, user: @user %>
<!--User一覧ページ-->
<h2><strong>Users</strong></h2>

<tabel>
  <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
  
  <% @users.each do |user| %>
    <td><%= image_tag user.get_profile_image(100,100) %></td>
    <td><%= user.name %></td>
    <td><%= link_to 'Show', user_path(user.id) %></td>
  </tr>
  <% end %>
</tabel>
